{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "project": ["**/*.{ts,js}"],
  "entry": [
    ".config/**/*.ts",
    ".tests/**/*.ts",
    "*.{config,cfg}.{ts,js}",
    "src/mod.ts",
    "src/**/*-mod.ts",
    "example/**/*.ts"
  ],
  "ignore": [
    "node_modules",
    "dist-jsr",
    "dist-npm",
    "dist-libs",
    "src/types.ts",
    "tests/relinka.test.ts",
    "src/deprecated/impl-old.ts",
    "src/libs/reprint/types.ts",
    "src/deprecated/utils/format-new.ts"
  ],
  "ignoreBinaries": ["dler"]
}
