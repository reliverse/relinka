{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "project": ["**/*.{ts,js}"],
  "entry": [
    "*.{config,cfg}.{ts,js}",
    "src/main.ts",
    "src/**/*-main.ts",
    "examples/**/*.ts"
  ],
  "ignore": [
    "node_modules",
    "dist-jsr",
    "dist-npm",
    "dist-libs",
    "src/types.ts",
    "tests/relinka.test.ts",
    "src/deprecated/impl-old.ts"
  ],
  "ignoreBinaries": ["relidler"]
}
