    "@types/bun": "^1.1.16",
    "@types/chalk-animation": "^1.6.3",
    "@types/eslint__js": "^8.42.3",
    "@types/figlet": "^1.7.0",
    "@types/fs-extra": "^11.0.4",
    "@types/mute-stream": "^0.0.4",
    "@types/node": "^22.10.6",
    "@types/sentencer": "^0.2.3",
    "@types/signal-exit": "^3.0.4",
    "@types/strip-comments": "^2.0.4",
    "@types/window-size": "^1.1.4",
    "c12": "^2.0.1",
    "citty": "^0.1.6",
