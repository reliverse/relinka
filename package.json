{
  "name": "@reliverse/relinka-cli",
  "version": "1.3.7",
  "type": "module",
  "description": "@reliverse/relinka is your next favorite logging library — built to make your terminal (and browser console — soon) output look good, stay clean, and be actually helpful. It’s styled, structured, and smart. Oh, and it works with configs, files, and colors out of the box.",
  "scripts": {
    "dev": "bun examples/e-main.ts",
    "latest": "bun update --latest && bun check",
    "check": "tsc --noEmit && eslint --cache --fix . && biome check --fix --unsafe . && knip",
    "agg": "relidler tools --tool agg --input src/libs/relinka/relinka-impl --out src/libs/relinka/relinka-main.ts --recursive --named --strip src/libs/relinka",
    "pub": "relidler",
    "vi": "vitest"
  },
  "dependencies": {
    "@reliverse/relico": "^1.1.0",
    "@reliverse/runtime": "^1.0.3",
    "c12": "^3.0.2",
    "defu": "^6.1.4",
    "fs-extra": "^11.3.0",
    "pathe": "^2.0.3",
    "printj": "^1.3.1",
    "std-env": "^3.8.1",
    "string-width": "^7.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@eslint/js": "^9.23.0",
    "@reliverse/relidler-cfg": "^1.1.3",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/bun": "^1.2.8",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.13.17",
    "@types/sentencer": "^0.2.3",
    "eslint": "^9.23.0",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-perfectionist": "^4.11.0",
    "jiti": "^2.4.2",
    "knip": "^5.46.4",
    "sentencer": "^0.2.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.29.0",
    "vitest": "^3.1.1"
  }
}
